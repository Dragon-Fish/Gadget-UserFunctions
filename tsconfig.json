    "target": "es5",
    "module": "umd",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "lib": ["esnext", "dom"],
    // "resolveJsonModule": true,
    "plugins": [],
    "outDir": "lib"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "./node_modules/types-mediawiki"
  ]
}
